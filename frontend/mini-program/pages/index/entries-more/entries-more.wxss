/* pages/index/entries-more/entries-more.wxss */

.container {
  background: #f5f5f5;
  min-height: 100vh;
  padding: 20rpx;
}

.card {
  background: #fff;
  border-radius: var(--r-card);
  overflow: hidden;
}

.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 28rpx 24rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.row:last-child {
  border-bottom: none;
}

.name {
  font-size: 28rpx;
  color: #111827;
}

.arrow {
  width: 32rpx;
  height: 32rpx;
  opacity: 0.7;
}

.empty {
  padding: 40rpx 24rpx;
  color: #6b7280;
  font-size: 26rpx;
}


